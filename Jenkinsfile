                echo 'Starting to Build from GitHub'
            }
        }
        stage('Test') {
            steps {
                echo 'Hello World from Sanim16 in Github'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Completed Build'
            }
        }
    }
}
